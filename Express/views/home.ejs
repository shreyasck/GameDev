<html>
<head>

	<title><%= title %></title>
</head>




<body>
<div id="wrap">
	<div id="regbar">
		<div id="navthing">
			<h2><a href="#" id="loginform">User Info</a> | <a href="#">Sign Out</a></h2>
			<div class="login">
				<div class="arrow-up"></div>
				<div class="formholder">
					<div class="randompad">
						<fieldset>



							<% if (!user) { %>
							<p>Welcome! Please <a href="/signin">log in</a>.</p>
							<% } else { %>
							<p>Hello, <%= user.username %>. View your <a href="/profile">profile</a>.</p>
							<% } %>
							<br/>
							<p>
								ID: <%= user.id %><br/>
								Username: <%= user.username %><br/>
								Name: <%= user.displayName %><br/>
								<% if (user.emails) { %>
								Email: <%= user.emails[0].value %><br/>
								<% } %>
							</p>
							<a href="/startGame">Start Game</a>

						</fieldset>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>


<script src="home.js"></script>
<link rel="stylesheet" href="home.css">
<link rel="stylesheet" href="css/style.css">

</body>






</html>
